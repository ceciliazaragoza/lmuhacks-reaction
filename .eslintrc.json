{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "root": true,
  "ignorePatterns": ["node_modules/*", "**/*.js"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-unused-vars": ["off"],
    "@typescript-eslint/no-unused-vars": "off"
  }
}
